
set angles degrees

e  = 10e3	# [V/m] (100v/cm)

r  = 10e-6	# [m]

d  = 25e-9	

so = 0.15	# [S/m]

sm = 5e-7

si = 0.2

num = 3*so * (3*d*(r**2)*si + (3*(d**2)*r - (d**3)) * (sm - si))

den = 2*(r**3) * (sm + 2*so) * (sm + .5*si) - 2 * ((r - d)**3) * (so - sm) * (si - sm)

f = num / den

itv_teo(x) = f * e * r * cos(x)

set terminal png

set xlabel "Tita [º]"

set xrange [0 : 180]
# set yrange [25:100]

set output "itv25-teo.png"

set ylabel "ITV [V]"

plot itv_teo(x) with lines linestyle 1 title "ITV teórico d = 25 nm"

set output "itv25.png"

set ylabel "ITV [V]"

plot "itv25.csv" using ($1):($2) with lines linestyle 1 title "ITV d = 25 nm"

set output "itv-error-25.png"

set ylabel "Error ITV [V]"

plot "itv25.csv" using ($1):(abs($2 - itv_teo($1))) with points title "Error ITV d = 25 nm"

